<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.studio.cai.mysuperdemo">

    <!--权限-->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".MyApplication"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.GuideActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity
            android:name=".ui.BannerActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity
            android:name=".ui.SpringActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity
            android:name=".ui.MoreTabActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity
            android:name=".ui.TabMainActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity android:name=".ui.WaterFallActivity"/>
        <activity android:name=".ui.BannerSimpleActivity"/>
        <activity android:name=".ui.BannerSimpleActivity2"/>
        <activity android:name=".widget.photo.PhotoBrowse"/>
        <activity android:name=".widget.photo.ViewPagerActivity"/>
        <activity android:name=".retrofit.RetrofitActivity"/>
        <activity android:name=".rxjava_retrofit.ElementaryActivity"/>

    </application>

</manifest>